{
  "name": "deadliner",
  "version": "0.1.5",
  "description": "",
  "main": "index.js",
  "scripts": {
    "mstart": "concurrently -k \"BROWSER=none npm start\" \"npm run wait-electron-start\"",
    "wait-electron-start": "wait-on http://192.168.0.104:3000 && electron .",
    "electron-start": "electron .",
    "buildW": "electron-packager . --out=build --overwrite --platform=win32",
    "buildL": "electron-packager . --out=build --overwrite --platform=linux",
    "electron-build": "electron-builder --win --x64",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "icon": "undefind",
      "legalTrademarks": "undefind"
    },
    "linux": {
      "target": [
        {
          "target": "deb",
          "arch": [
            "x64"
          ]
        }
      ],
      "maintainer": "me"
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "date-events": "^1.0.1",
    "electron-custom-notifications": "^8.0.5",
    "luxon": "^3.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "wait-on": "^7.0.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "concurrently": "^8.0.1",
    "electron": "^23.2.1",
    "nodemon": "^2.0.22"
  }
}
